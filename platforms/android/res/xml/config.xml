<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gmail.mikankui" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="WakeupPlugin">
        <param name="android-package" value="org.nypr.cordova.wakeupplugin.WakeupPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Vibration">
        <param name="android-package" value="org.apache.cordova.vibration.Vibration" />
    </feature>
    <feature name="AlarmPlugin">
        <param name="android-package" value="com.uniclau.alarmplugin.AlarmPlugin" />
    </feature>
    <feature name="SleepTimerPlugin">
        <param name="android-package" value="org.nypr.cordova.sleeptimerplugin.SleepTimerPlugin" />
    </feature>
    <feature name="TimerPlugin">
        <param name="android-package" value="de.schchr.cordova.plugin.timers.TimerPlugin" />
    </feature>
    <name>WakeMeUp</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
